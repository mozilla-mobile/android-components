{
  "manifest_version": 2,
  "name": "Mozilla Android Components - Browser Icons",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["icons.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "geckoViewAddons",
    "nativeMessaging"
  ]
}
